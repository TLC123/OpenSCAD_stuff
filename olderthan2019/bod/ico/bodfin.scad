mesh=bod();
depth=.5;
pullback=.25;
points=mesh[0];
faces=mesh[1];


difference(){
union(){
    polyhedron(points,faces);
    sphere(.001);
}

 union(){
    for(f=faces){
        hcface=[for(p=f)points[p]];
        c=([1,1,1]*hcface)/3;
        miniface=lerp(hcface,[c,c,c],pullback);
        tinyface=lerp(miniface,[c,c,c],pullback);
        n= face_normal(hcface);
        n3=[n,n,n];
        difference(){
        hull(){
            color("red")polyhedron(miniface+n3*depth,[[0,1,2]]);
            color("red")polyhedron(tinyface+n3*-depth,[[0,1,2]]);
        }
        }
    }

}
}




function un(v) = v / max(norm(v), 1e-64) * 1;
function clamp(a,b=0,c=1) =  min(max(a,min(b,c)),max(b,c));
function lerp(start,end,bias) = (end * bias + start * (1 - bias));
function rnd(a,b)=a+(rands(0,1,1)[0])*(b-a);




function face_normal(v)= len(v)>3?un(face_normal([v[0],v[1],v[2]])
    +face_normal(concat([v[0]],[for(i=[2:len(v)-1])v[i]]))):let(u=v[0]-v[1],w=v[0]-v[2])
    un([u[1]*w[2]-u[2]*w[1],u[2]*w[0]-u[0]*w[2],u[0]*w[1]-u[1]*w[0]]);




